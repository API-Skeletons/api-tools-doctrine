    },
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": ">=2.2",
        "zfcampus/zf-apigility": "*",
        "zfcampus/zf-rpc": "*",
        "doctrine/common": ">=2.4.x-dev",
