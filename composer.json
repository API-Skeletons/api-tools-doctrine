{
    "name": "zfcampus/zf-apigility-doctrine",
    "type": "library",
    "description": "Doctrine for Apigility Admin and Server",
    "keywords": [
        "apigility",
        "doctrine",
        "api client"
    ],
    "homepage": "http://www.apigility.com/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Tom Anderson",
            "email": "tanderson@soliantconsulting.com"
        }
    ],
    "require-dev": {
        "zfcampus/zf-apigility-admin": "dev-master",
        "phpunit/phpunit": "3.7.*",
        "doctrine/doctrine-orm-module": "*",
        "doctrine/doctrine-mongo-odm-module": "*",
        "doctrine/mongodb-odm": "1.0.0-beta9@dev"
    },
    "require": {
        "php": ">=5.4",
        "zendframework/zendframework": "dev-develop",
        "zfcampus/zf-apigility": "dev-master",
        "zfcampus/zf-configuration": "dev-master"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-0": {
            "Apigility\\Doctrine": "src/",
            "Apigility\\Doctrine\\Admin": "src/",
            "Apigility\\Doctrine\\Server": "src/"
        }
    }
}
