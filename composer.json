    "keywords": [
        "apigility",
        "api client"
    ],
    "homepage": "http://www.soliantconsulting.com/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Tom Anderson",
            "email": "tanderson@soliantconsulting.com"
        }
