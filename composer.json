{
    "name": "soliantconsulting/soliantconsulting-apigility",
    "type": "library",
    "description": "An ORM Apigility Client based on Doctrine Common",
    "keywords": [
        "apigility",
        "api client"
    ],
    "homepage": "http://www.soliantconsulting.com/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Tom Anderson",
            "email": "tanderson@soliantconsulting.com"
        }
    ],
    "require-dev": {
        "zfcampus/zf-apigility-admin": "dev-master",
        "phpunit/phpunit": "3.7.*",
        "doctrine/doctrine-orm-module": "*",
        "doctrine/doctrine-mongo-odm-module": "*",
        "doctrine/mongodb-odm": "1.0.0-beta9@dev"
    },
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "dev-develop",
        "zfcampus/zf-apigility": "dev-master",
        "zfcampus/zf-configuration": "dev-master",
        "doctrine/common": ">=2.4.x-dev",
        "ocramius/proxy-manager": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-0": {
            "SoliantConsulting\\Apigility": "src/",
            "SoliantConsulting\\Apigility\\Admin": "src/",
            "SoliantConsulting\\Apigility\\Server": "src/",
            "SoliantConsulting\\Apigility\\Client": "src/"
        }
    }
}
